export alias ytnpl = yt-dlp --no-playlist --restrict-filenames
export alias ytp = ytnpl --write-subs --write-auto-subs --format 244+299
export alias ytpp = ytnpl --write-subs --write-auto-subs --format 247+299
export alias yts = ytnpl --write-subs --write-auto-subs --format worstaudio --extract-audio
export alias ytm = ytnpl --format bestaudio --extract-audio
